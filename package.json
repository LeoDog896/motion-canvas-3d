{
  "name": "motion-canvas-3d",
  "version": "0.3.0",
  "description": "3D renderer for motion-canvas",
  "files": [
    "dist"
  ],
  "main": "./dist/motion-canvas-3d.umd.cjs",
  "module": "./dist/motion-canvas-3d.js",
  "types": "./dist/motion-canvas-3d.d.ts",
  "exports": {
    ".": {
      "import": "./dist/motion-canvas-3d.js",
      "require": "./dist/motion-canvas-3d.umd.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "example": "vite",
    "format": "npx prettier . --write",
    "build": "vite build --config vite.config.lib.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "npm run build && npm run lint"
  },
  "keywords": [
    "motion-canvas",
    "motion",
    "canvas",
    "3d",
    "three.js"
  ],
  "homepage": "https://github.com/LeoDog896/motion-canvas-3d#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeoDog896/motion-canvas-3d.git"
  },
  "bugs": {
    "url": "https://github.com/LeoDog896/motion-canvas-3d/issues"
  },
  "author": "LeoDog896 <leodog896@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@motion-canvas/2d": "^3.11.0",
    "@motion-canvas/core": "^3.11.0",
    "@motion-canvas/ffmpeg": "^1.1.1",
    "@motion-canvas/ui": "^3.11.0",
    "@motion-canvas/vite-plugin": "^3.11.0",
    "@types/three": "^0.158.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "@motion-canvas/2d": "3.x",
    "@motion-canvas/core": "3.x"
  },
  "dependencies": {
    "three": "^0.158.0"
  }
}
